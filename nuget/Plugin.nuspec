<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="2.8.1">
    <id>Xam.Plugins.TextToSpeech</id>
    <version>$version$</version>
    <title>Text to Speech Plugin for Xamarin and Windows</title>
    <authors>James Montemagno</authors>
    <owners>James Montemagno</owners>
    <licenseUrl>https://github.com/jamesmontemagno/TextToSpeechPlugin/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/jamesmontemagno/TextToSpeechPlugin</projectUrl>
    <iconUrl>http://refractored.com/images/tts_icon_large.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>
      Cross platform Text-to-Speech for Xamarin and Windows.
    </summary>
    <releaseNotes>
      See: https://github.com/jamesmontemagno/TextToSpeechPlugin/blob/master/CHANGELOG.md
    </releaseNotes>
    <description>
      Xamarin and Windows plugin to perform text to speech functionality.
      Adjustable pitch, speak rate, locale, and more.
    </description>
    <tags>xamarin, xamarin.forms, android, ios, windows phone, windows store, winrt, universal apps, mac, text to speech, tts, texttospeech</tags>
    <dependencies>
      <group targetFramework="net">
      </group>
      <group targetFramework="win">
      </group>
      <group targetFramework="wp">
      </group>
      <group targetFramework="wpa">
      </group>
      <group targetFramework="netstandard1.0">
        <dependency id="NETStandard.Library" version="1.6.1" />
      </group>
      <group targetFramework="MonoAndroid10">
      </group>
      <group targetFramework="Xamarin.iOS10">
      </group>
      <group targetFramework="Xamarin.Mac20">
      </group>
      <group targetFramework="portable-net45+win+wpa81+wp80">
      </group>
      <group targetFramework="uap">
      </group>
      <group targetFramework="dotnet">
      </group>
      <group targetFramework="xamarintvos">
      </group>
      <group targetFramework="xamarinwatchos">
      </group>
      <group targetFramework="tizen40">
      </group>
    </dependencies>
  </metadata>
  <files>
    <!--Core-->
    <file src="src\TextToSpeech.Plugin\bin\Release\netstandard1.0\Plugin.TextToSpeech.*" target="lib\netstandard1.0"/>

    <!--Xamarin.Android-->
    <file src="src\TextToSpeech.Plugin.Android\bin\Release\Plugin.TextToSpeech.*" target="lib\MonoAndroid10" />

    <!--Xamarin.iOS Unified-->
    <file src="src\TextToSpeech.Plugin.iOS\bin\iPhone\Release\Plugin.TextToSpeech.*" target="lib\Xamarin.iOS10" />

	  <!--Xamarin.tvOS-->
	  <file src="src\TextToSpeech.Plugin.tvOS\bin\Release\Plugin.TextToSpeech.*" target="lib\Xamarin.tvOS10" />

		<!--Xamarin.Mac-->
    <file src="src\TextToSpeech.Plugin.Mac\bin\Release\Plugin.TextToSpeech.*" target="lib\Xamarin.Mac20" />

    <!--UWP-->
    <file src="src\TextToSpeech.Plugin.UWP\bin\Release\Plugin.TextToSpeech.*" target="lib\UAP10" />

    <!--Tizen-->
    <file src="src\TextToSpeech.Plugin.Tizen\bin\Release\tizen40\Plugin.TextToSpeech.*" target="lib\tizen40" />

  </files>
</package>